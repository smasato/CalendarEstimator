{
  "compilerOptions": {
    "baseUrl": "./",
    "rootDir": ".",
    "outDir": "./dist",
    "strict": true,
    "sourceMap": true,
    "target": "ESNext",
    "allowJs": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
    "esModuleInterop": true,
    "noEmit": true,
    "noImplicitAny": false,
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "plotly.js": ["node_modules/@types/plotly.js"]
    },
    "types": ["@types/node", "@nuxt/types", "plotly.js", "vuetify"],
    "typeRoots": [
      "types",
      "./node_modules/@types",
      "./node_modules/@stdlib/types"
    ]
  },
  "exclude": ["node_modules"],
  "vueCompilerOptions": {
    "preserveWhitespace": false,
    "target": 2.7
  }
}
