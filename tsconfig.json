{
  "compilerOptions": {
    "baseUrl": "./",
    "rootDir": ".",
    "outDir": "./dist",
    "strict": true,
    "sourceMap": true,
    "target": "ESNext",
    "allowJs": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
    "esModuleInterop": true,
    "noEmit": true,
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    },
    "types": ["@types/node", "@nuxt/types"],
    "typeRoots": ["types", "./node_modules/@types"]
  },
  "exclude": ["node_modules"],
  "vueCompilerOptions": {
    "preserveWhitespace": false,
    "target": 2.7
  }
}
